server {
    listen 80;
    server_name example.com;

    location = /favicon.ico { access_log off; log_not_found off; }
    location /static/ {
        root /home/django/sample;
    }

    location / {
        include         uwsgi_params;
        uwsgi_pass      unix:/home/django/sample/sample.sock;
    }
}